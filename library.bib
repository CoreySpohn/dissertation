@article{motalebiHARPSNRocky2015,
  title = {The {{HARPS-N Rocky Planet Search}}. {{I}}. {{HD}} 219134 b: {{A}} Transiting Rocky Planet in a Multi-Planet System at 6.5 Pc from the {{Sun}}},
  shorttitle = {The {{HARPS-N Rocky Planet Search}}. {{I}}. {{HD}} 219134 b},
  author = {Motalebi, F. and Udry, S. and Gillon, M. and Lovis, C. and Ségransan, D. and Buchhave, L. A. and Demory, B. O. and Malavolta, L. and Dressing, C. D. and Sasselov, D. and Rice, K. and Charbonneau, D. and Collier Cameron, A. and Latham, D. and Molinari, E. and Pepe, F. and Affer, L. and Bonomo, A. S. and Cosentino, R. and Dumusque, X. and Figueira, P. and Fiorenzano, A. F. M. and Gettel, S. and Harutyunyan, A. and Haywood, R. D. and Johnson, J. and Lopez, E. and {Lopez-Morales}, M. and Mayor, M. and Micela, G. and Mortier, A. and Nascimbeni, V. and Philips, D. and Piotto, G. and Pollacco, D. and Queloz, D. and Sozzetti, A. and Vanderburg, A. and Watson, C. A.},
