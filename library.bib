@article{doi:10.1029/2010GL045229,
  title = {J2: {{An}} Evaluation of New Estimates from {{GPS}}, {{GRACE}}, and Load Models Compared to {{SLR}}},
  author = {Lavallée, D A and Moore, P and Clarke, P J and Petrie, E J and {van Dam}, T and King, M A},
  year = {2010},
  journal = {Geophysical Research Letters},
  volume = {37},
  number = {22},
  doi = {10.1029/2010GL045229},
  abstract = {Changes in J2, resulting from past and present changes in Earth's climate, are traditionally observed by Satellite Laser ranging (SLR). Assuming an elastic Earth, it is possible to infer changes in J2 from changes in Earth's shape observed by GPS. We compare estimates of non-secular J2 changes from GPS, SLR, GRACE, and a load model. The GPS and SLR annual signals agree but are different (16\%) to the load model. Subtraction of the load model removes the annual variation from GPS, SLR, and GRACE, and the semi-annual variation in GPS. The GPS and SLR long-term signals are highly correlated, but GPS is better correlated with the loading model. Subtraction of the load model removes the 1998 anomaly from the GPS J2 series but not completely from the SLR J2 series, suggesting that the SLR anomaly may not be entirely due to mass re-distribution as has been presumed.},
  keywords = {dynamic oblateness,GPS,GRACE,J2,loading,SLR}
}
