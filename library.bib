@article{fischerFivePlanetsOrbiting2008,
  title = {Five {{Planets Orbiting}} 55 {{Cancri}}},
  author = {Fischer, Debra A. and Marcy, Geoffrey W. and Butler, R. Paul and Vogt, Steven S. and Laughlin, Greg and Henry, Gregory W. and Abouav, David and Peek, Kathryn M. G. and Wright, Jason T. and Johnson, John A. and McCarthy, Chris and Isaacson, Howard},
