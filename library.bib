@misc{bomzeUncertaintyPreferences2022,
  title = {Uncertainty {{Preferences}} in {{Robust Mixed-Integer Linear Optimization}} with {{Endogenous Uncertainty}}},
  author = {Bomze, Immanuel and Gabl, Markus},
