@article{turnbullSearchHabitable2012,
  title = {The {{Search}} for {{Habitable Worlds}}. 1. {{The Viability}} of a {{Starshade Mission}}},
  author = {Turnbull, Margaret C. and Glassman, Tiffany and Roberge, Aki and Cash, Webster and Noecker, Charley and Lo, Amy and Mason, Brian and Oakley, Phil and Bally, John},
