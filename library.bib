@article{Ruffio2018,
  title = {A {{Bayesian Framework}} for {{Exoplanet Direct Detection}} and {{Non-detection}}},
  author = {Ruffio, Jean-Baptiste and Mawet, Dimitri and Czekala, Ian and Macintosh, Bruce and De Rosa, Robert J. and Ruane, Garreth and Bottom, Michael and Pueyo, Laurent and Wang, Jason J. and Hirsch, Lea and Zhu, Zhaohuan and Nielsen, Eric L.},
