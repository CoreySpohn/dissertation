@article{sahlmannAstrometricOrbitLowmass2013,
  title = {Astrometric Orbit of a Low-Mass Companion to an Ultracool Dwarf},
  author = {Sahlmann, J. and Lazorenko, P. F. and Segransan, D. and Martin, E. L. and Queloz, D. and Mayor, M. and Udry, S.},
