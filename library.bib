  author = {Mennesson, B. and {Millan-Gabet}, R. and Serabyn, E. and Colavita, M. M. and Absil, O. and Bryden, G. and Wyatt, M. and Danchi, W. and Defrère, D. and Doré, O. and Hinz, P. and Kuchner, M. and Ragland, S. and Scott, N. and Stapelfeldt, K. and Traub, W. and Woillez, J.},
