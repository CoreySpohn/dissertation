@article{bryanStatisticsLongPeriod2016,
  title = {Statistics of {{Long Period Gas Giant Planets}} in {{Known Planetary Systems}}},
  author = {Bryan, Marta L. and Knutson, Heather A. and Howard, Andrew W. and Ngo, Henry and Batygin, Konstantin and Crepp, Justin R. and Fulton, B. J. and Hinkley, Sasha and Isaacson, Howard and Johnson, John A. and Marcy, Geoffry W. and Wright, Jason T.},
