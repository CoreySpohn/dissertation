@article{dulzJointRadialVelocity2020,
  title = {Joint {{Radial Velocity}} and {{Direct Imaging Planet Yield Calculations}}. {{I}}. {{Self-consistent Planet Populations}}},
  author = {Dulz, Shannon D. and Plavchan, Peter and Crepp, Justin R. and Stark, Christopher and Morgan, Rhonda and Kane, Stephen R. and Newman, Patrick and Matzko, William and Mulders, Gijs D.},
  year = {2020},
  month = apr,
  journal = {The Astrophysical Journal},
