@article{watsonEstimatingMassesExtrasolar2010,
  title = {Estimating the Masses of Extra-Solar Planets},
  author = {Watson, C. A. and Littlefair, S. P. and Cameron, A. Collier and Dhillon, V. S. and Simpson, E. K.},
  year = {2010},
  month = nov,
